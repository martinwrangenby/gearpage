{
  "name": "react-playground",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.21.4",
    "dotenv": "^16.0.0",
    "jest": "^27.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:repair": "eslint . --fix",
    "start": "node scripts/devServerBootstrap.js && react-scripts start",
    "build": "react-scripts build",
    "e2e": "npx playwright test",
    "e2e:headful": "HEADFUL=true npm run e2e",
    "e2e:ci": "REACT_APP_FRONTEND=http://localhost:3000/ start-server-and-test 3000 e2e",
    "test": "react-scripts test",
    "cleanup": "rimraf e2e/results"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.20.0",
    "eslint": "^8.11.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-playwright": "^0.8.0",
    "expect-playwright": "^0.3.0",
    "husky": "^4.3.7",
    "jest-playwright-preset": "^1.4.3",
    "playwright": "^1.20.0",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.11.7"
  }
}

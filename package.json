{
  "name": "gear-page",
  "version": "0.1.0",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.0",
    "firebase": "^9.6.9",
    "googleapis": "^99.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-firebase-hooks": "^5.1.1",
    "react-router-dom": "6.4.3"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:repair": "eslint . --fix",
    "start": "node scripts/devServerBootstrap.js && react-scripts start",
    "build": "react-scripts build",
    "e2e": "npx playwright test",
    "e2e:browserstack": "BROWSERSTACK=true browserstack-node-sdk playwright test --config=./playwright.config.browserstack.js",
    "e2e:ci": "CI=true npm run e2e",
    "test": "react-scripts test",
    "cleanup": "rimraf e2e/results log",
    "danger:ci": "danger ci",
    "danger:local": "danger pr"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "1.47.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "browserstack-node-sdk": "^1.34.49",
    "danger": "^11.0.2",
    "eslint": "^8.11.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-playwright": "^0.8.0",
    "husky": "^4.3.7",
    "jest": "^29.7.0",
    "msw": "^2.9.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2"
  }
}
